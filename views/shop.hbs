

<h1>Here are my goods:</h1>
<form method="POST" ACTION="/shopsearch">
<input type="text" name="pattern">
<input type="submit" name="submit" value="Search">
</form>
<div style="border:1px black solid">
{{#each result1.rows}}
  <div class="shopitem">
    <h2>{{this.name}}</h2><br>
    <img src="/img/box.png" style="width: 100px">
    <h2>For only:<br>{{this.price}}</h2>
  </div>
{{/each}}
</div>
<br>
<h1>
<a href="/shop/{{prev}}">&lt;&lt;</a>
&nbsp;&nbsp;&nbsp;&nbsp;
<a href="/shop/{{next}}">&gt;&gt;</a>
</h1>
<h4>Choose wisely...</h4>
